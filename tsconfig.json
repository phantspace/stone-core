{
  "compilerOptions": {
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "downlevelIteration": true,
    "removeComments": true,
    "target": "es5",
    "module": "ESNext",
    "strict": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}
